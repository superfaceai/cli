{
  "name": "@superfaceai/cli",
  "version": "0.0.25",
  "description": "Superface CLI utility",
  "main": "dist/index.js",
  "repository": "https://github.com/superfaceai/cli.git",
  "author": "Superface Team",
  "private": false,
  "bin": {
    "superface": "bin/superface",
    "sf": "bin/superface"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.release.json --outDir dist",
    "test": "jest",
    "test:clean": "jest --clear-cache && jest",
    "lint": "eslint src/",
    "lint:fix": "yarn lint --fix",
    "format": "prettier -c src/",
    "format:fix": "prettier --write src/",
    "prepush": "yarn build && yarn test && yarn lint && yarn format"
  },
  "devDependencies": {
    "@types/concat-stream": "^1.6.1",
    "@types/debug": "^4.1.7",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^27.0.2",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "concat-stream": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.2.5",
    "mockttp": "^2.2.4",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@superfaceai/service-client": "0.0.24",
    "@superfaceai/ast": "0.0.31",
    "@superfaceai/parser": "0.0.22",
    "@superfaceai/one-sdk": "0.0.38-beta.0",
    "chalk": "^4.1.2",
    "cross-fetch": "^3.1.4",
    "debug": "^4.3.2",
    "inquirer": "^8.2.0",
    "netrc-parser": "^3.1.6",
    "open": "^8.3.0",
    "rimraf": "^3.0.2",
    "typescript": "4.3.5"
  },
  "oclif": {
    "commands": "dist/commands",
    "bin": "superface",
    "hooks": {
      "init": "dist/hooks/init"
    }
  }
}
